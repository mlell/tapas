#!/usr/bin/env bash
## Launch the script preview.
## This is DEPRECATED. You should rather use plot_mutation_probabilities

# This launcher was automatically generated by
# gen/gen-launchers.sh

## ----- Launcher follows -----------------------------

# Usage: sh_launch SCRIPT BASEDIR
#
# Launch a shell script. SCRIPT is a path relative to BASEDIR
function sh_launch(){
    scriptname="$1"
    basedir="$2"
    shift 2

    "${basedir}/$scriptname" "$@"
}


# Directory this script lives in
thisdir="$(dirname "$(readlink -f "$0")")"
# Launch the script and forward all command line arguments
echo 'Using the function preview is DEPRECATED.'\
     'Use plot_mutation_probabilities instead.' >&2 
sh_launch plot_mutation_probabilities "${thisdir}" "$@"
