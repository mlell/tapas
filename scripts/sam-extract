#!/usr/bin/env bash
## Launch the script sam-extract.
## This is DEPRECATED. You should rather use sam_extract

# This launcher was automatically generated by
# gen/gen-launchers.sh

## ----- Launcher follows -----------------------------

# Usage: sh_launch SCRIPT BASEDIR
#
# Launch a shell script. SCRIPT is a path relative to BASEDIR
function sh_launch(){
    scriptname="$1"
    basedir="$2"
    shift 2

    "${basedir}/$scriptname" "$@"
}


# Directory this script lives in
thisdir="$(dirname "$(readlink -f "$0")")"
# Launch the script and forward all command line arguments
echo 'Using the function sam-extract is DEPRECATED.'\
     'Use sam_extract instead.' >&2 
sh_launch sam_extract "${thisdir}" "$@"
