#!/bin/sh
## Launch the script plot-read-fate.
## This is DEPRECATED. You should rather use plot_read_fate

# This launcher was automatically generated by
# gen/gen-launchers.sh

## ----- Launcher follows -----------------------------

# Usage: sh_launch SCRIPT BASEDIR
#
# Launch a shell script. SCRIPT is a path relative to BASEDIR
function sh_launch(){
    scriptname="$1"
    basedir="$2"
    shift 2

    "${basedir}/$scriptname" "$@"
}


# Directory this script lives in
thisdir="$(dirname "$(readlink -f "$0")")"
# Launch the script and forward all command line arguments
echo 'Using the function plot-read-fate is DEPRECATED.'\
     'Use plot_read_fate instead.' >&2 
sh_launch plot_read_fate "${thisdir}" "$@"
